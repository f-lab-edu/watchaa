{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2017",
    "loose": false
  },
  "module": {
    "type": "es6",
    "noInterop": false
  },
  "sourceMaps": true,
  "minify": true
}
