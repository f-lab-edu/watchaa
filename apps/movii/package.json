    "@types/dotenv-webpack": "^7.0.8",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-router-dom": "^5.3.3",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "catalog:",
    "jiti": "^2.6.0",
    "postcss": "^8.5.6",
