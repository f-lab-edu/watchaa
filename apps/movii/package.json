{
  "name": "@movii/web",
  "version": "1.0.0",
  "private": true,
  "description": "watcha clone coding project",
  "type": "module",
  "prettier": "@movii/prettier-config",
  "scripts": {
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "dev": "cross-env NODE_ENV=development rspack serve --config rspack.dev.mjs",
    "build": "cross-env NODE_ENV=production rspack --config rspack.prod.mjs",
    "build:preview": "pnpm run build && npx serve dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' '*.mjs'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' '*.mjs' --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@movii/icons": "workspace:*",
    "@movii/image": "workspace:*",
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "movii-carousel": "workspace:*",
    "radash": "^12.1.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.9.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@movii/eslint-config": "workspace:*",
    "@movii/prettier-config": "workspace:*",
