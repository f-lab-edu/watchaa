{
  "name": "movii-carousel",
  "version": "1.0.2",
  "description": "A customizable React carousel component",
  "private": false,
  "type": "module",
  "prettier": "@movii/prettier-config",
  "exports": {
    ".": "./src/index.tsx",
    "./carousel.css": "./src/carousel.css"
  },
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs"
      },
      "./carousel.css": "./dist/carousel.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "carousel",
    "slider",
    "component"
  ],
  "author": "Moon-Sangho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/f-lab-edu/movii.git",
    "directory": "packages/carousel"
  },
  "peerDependencies": {
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@movii/eslint-config": "workspace:*",
    "@movii/prettier-config": "workspace:*",
    "@movii/typescript-config": "workspace:*",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/react": "catalog:",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "catalog:",
    "eslint-plugin-storybook": "10.0.6",
    "postcss-preset-env": "^10.4.0",
    "prettier": "catalog:",
    "react": "catalog:",
    "storybook": "^10.0.6",
    "tailwindcss": "^4.1.16",
    "typescript": "catalog:",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4"
  }
}
